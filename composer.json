{
  "name": "asciito/simple-generators",
  "description": "A very simple package to handle text and image generation from AI tools",
  "license": "MIT",
  "type": "package",
  "authors": [
    {
      "name": "Ayax CÃ³rdova",
      "email": "ayax.cordova@aydev.com",
      "homepage": "https://aydev.mx"
    }
  ],
  "require": {
    "illuminate/collections": "^10.47",
    "nyholm/psr7": "^1.8",
    "openai-php/client": "^0.8.4",
    "symfony/http-client": "^7.0"
  },
  "require-dev": {
    "laravel/pint": "^1.14",
    "pestphp/pest": "^v2.34.1"
  },
  "autoload": {
    "psr-4": {
      "Asciito\\SimpleGenerators\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Asciito\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "version": "v0.0.1"
}
